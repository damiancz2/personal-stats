        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HelloWorld">
        <activity android:name="com.damiancz2.personalstats.activities.EditQuestionnaireActivity"/>
        <activity android:name="com.damiancz2.personalstats.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.damiancz2.personalstats.activities.NewQuestionnaireActivity" />
        <activity android:name="com.damiancz2.personalstats.activities.ViewQuestionsActivity" />
        <activity android:name="com.damiancz2.personalstats.activities.ViewAnswersActivity" />
        <activity android:name="com.damiancz2.personalstats.activities.answer.AnswerTextQuestionActivity" />
        <activity android:name="com.damiancz2.personalstats.activities.answer.AnswerTimeQuestionActivity" />
        <activity android:name="com.damiancz2.personalstats.activities.answer.AnswerNumberQuestionActivity" />
        <activity android:name="com.damiancz2.personalstats.activities.AddQuestionActivity" />
        <activity android:name="com.damiancz2.personalstats.activities.SubmittedActivity" />
    </application>

</manifest>